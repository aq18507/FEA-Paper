  file      = {C:Chang2018 - Design of Cellular Based Structures in Sandwiched Morphing Skin Via Topology Optimization.pdf:},
  groups    = {Morphing},
  keywords  = {Flexible skin, Morphing aircraft, Topology optimization, Two-dimensional morphing},
