@Article{Gornet2007,
  author    = {L. Gornet and S. Marguet and G. Marckmann},
  journal   = {Mechanics of Advanced Materials and Structures},
  title     = {Modeling of Nomex{\textregistered} Honeycomb Cores, Linear and Nonlinear Behaviors},
  year      = {2007},
  month     = nov,
  number    = {8},
  pages     = {589--601},
  volume    = {14},
  abstract  = {The purpose of this study is to develop tools dedicated to the design of sandwich panels involving Nomex honeycomb cores. Special attention is paid on the ability to perform full three dimensional calculations up to failure of such structures. In a first part, the determination of effective elastic properties of Nomex honeycomb cores is carried out thanks to strain based periodic homogenization technic. Using an equivalence in energy between a reel honeycomb and a fictious continuous medium, it becomes possible to evaluate the elastic behaviour of Nomex cores, starting from the knowledge of the behaviour of the constitutive paper. Next, relying on experimental observations, the strengths of Nomex honeycomb cores are evaluated with a linear Euler's buckling analysis. Results are compared with data coming from manufacturers, and give satisfaction. In order to carry out these two first studies, the NidaCore software has been developed using the finite element code Cast3M from CEA. Last part deals with the modeling of the nonlinear compressive response of Nomex cores. A model based on the thermodynamics of irreversible process is proposed and the identification technic detailed. Good agreement between experiemental data and computed values is obtained.},
  date      = {2017-01-03},
  day       = {3},
  doi       = {10.1080/15376490701675370},
  file      = {:Gornet2007 - Modeling of Nomex_ Honeycomb Cores, Linear and Nonlinear Behaviors.pdf:PDF},
  keywords  = {Nomex honeycomb core, homogenization, failure, nonlinear behaviour},
  publisher = {Informa {UK} Limited},
}

